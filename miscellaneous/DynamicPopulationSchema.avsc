      "type": "string"
    },
    {
      "name": "parent_object_id",
      "type": "string"
    },
    {
      "name": "population_type",
      "type": {
        "type": "enum",
        "name": "PopulationType",
        "symbols": [
          "OORT_CLOUD",
          "ASTEROID_BELT",
          "DEBRIS_FIELD",
          "DUST_CLOUD",
          "METEOROID_STREAM"
        ]
      }
