          {
            "name": "size_distribution",
            "type": {
              "type": "array",
              "items": "double"
            }
          },
          {
            "name": "composition_distribution",
            "type": {
              "type": "array",
              "items": "string"
            }
          },
          {
            "name": "albedo_range",
            "type": {
              "type": "array",
              "items": "double"
            }
          }
